[
    {
        "name": "Gemini GitHub Updater",
        "active": true,
        "nodes": [
            {
                "parameters": {
                    "httpMethod": "POST",
                    "path": "gemini-update",
                    "options": {}
                },
                "name": "Webhook",
                "type": "n8n-nodes-base.webhook",
                "typeVersion": 1,
                "position": [
                    100,
                    300
                ],
                "id": "webhook-node"
            },
            {
                "parameters": {
                    "resource": "file",
                    "operation": "update",
                    "fileId": {
                        "__rl": true,
                        "value": "=/GRICH_AI_BRAIN/GRICH_MASTER_LOG.md",
                        "mode": "name"
                    },
                    "updateFields": {
                        "appendData": "={{ JSON.stringify($json.body) }}"
                    }
                },
                "name": "Google Drive",
                "type": "n8n-nodes-base.googleDrive",
                "typeVersion": 3,
                "position": [
                    400,
                    300
                ],
                "id": "gdrive-node"
            }
        ],
        "connections": {
            "Webhook": {
                "main": [
                    [
                        {
                            "node": "Google Drive",
                            "type": "main",
                            "index": 0
                        }
                    ]
                ]
            }
        },
        "settings": {
            "executionOrder": "v1"
        }
    }
]